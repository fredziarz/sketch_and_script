<svg width="1920" height="1080" viewBox="0 0 1920 1080" xmlns="http://www.w3.org/2000/svg">
  <!-- Light gradient background -->
  <defs>
    <linearGradient id="archGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f8f8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d4d4d4;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1920" height="1080" fill="url(#archGrad)"/>
  
  <!-- Grid pattern -->
  <g stroke="#999" stroke-width="1" opacity="0.2">
    <!-- Vertical lines -->
    <line x1="320" y1="0" x2="320" y2="1080"/>
    <line x1="640" y1="0" x2="640" y2="1080"/>
    <line x1="960" y1="0" x2="960" y2="1080"/>
    <line x1="1280" y1="0" x2="1280" y2="1080"/>
    <line x1="1600" y1="0" x2="1600" y2="1080"/>
    <!-- Horizontal lines -->
    <line x1="0" y1="270" x2="1920" y2="270"/>
    <line x1="0" y1="540" x2="1920" y2="540"/>
    <line x1="0" y1="810" x2="1920" y2="810"/>
  </g>
  
  <!-- Abstract building shapes -->
  <g opacity="0.15" fill="#666">
    <rect x="1400" y="300" width="250" height="700"/>
    <rect x="1700" y="450" width="180" height="550"/>
    <rect x="200" y="600" width="150" height="400"/>
    <rect x="400" y="500" width="200" height="500"/>
  </g>
  
  <!-- Architectural elements -->
  <g stroke="#888" stroke-width="2" fill="none" opacity="0.25">
    <circle cx="960" cy="540" r="200"/>
    <rect x="100" y="100" width="300" height="200"/>
    <polyline points="1500,200 1650,100 1800,200"/>
  </g>
</svg>
